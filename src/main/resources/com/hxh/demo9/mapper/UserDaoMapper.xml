<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hxh.demo9.dao.UserDao">

    <select id="findAll" resultType="com.hxh.demo9.entity.User">
        SELECT
            `id`,
            `username`,
            `realname`,
            `password`,
            `gender`
        FROM
            `ems-thymeleaf`.`user`
    </select>
</mapper>